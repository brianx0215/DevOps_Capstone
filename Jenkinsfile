pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build files'){
            steps{
                echo 'build'
            }
        }
        stage('Lint'){
            steps{
                echo 'lint'
            }
        }
        stage('Build Docker'){
            steps{
                echo 'lint'
            }
        }
        stage('Deploy Docker'){
            steps{
                echo 'lint'
            }
        }
    }
}
